{
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "all",
    "importOrder": [
        "^@/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderParserPlugins": [
        "classProperties",
        "[\"decorators\", { \"decoratorsBeforeExport\": true }]"
    ],
    "plugins": [
        "@trivago/prettier-plugin-sort-imports"
    ]
}
